{
  "name": "tamagui-expo-template",
  "version": "1.0.0",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "wsl": "REACT_NATIVE_PACKAGER_HOSTNAME=$(netsh.exe interface ip show address \"Wi-Fi\" | grep 'IP Address' | sed -r 's/^.*IP Address:\\W*//') TAMAGUI_TARGET=native expo start",
    "start": "expo start",
    "android": "yarn expo run:android",
    "ios": "yarn expo run:ios",
    "web": "yarn expo start --web",
    "eject": "expo eject",
    "upgrade:tamagui": "manypkg upgrade tamagui && manypkg upgrade @tamagui && manypkg upgrade tamagui-loader"
  },
